
@model IEnumerable<Helpa.AspNetUser>
    @{
        Layout = null;
    }






    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <tr>
            <th>ProfileImage</th>
            <th>Description</th>
            <th>Email</th>
            <th>EmailConfirmed</th>
            <th>PhoneNumber</th>
            <th>PhoneNumberConfirmed</th>
            <th>UserName</th>
        </tr>
        @foreach (var v in Model)
            {
            <tr>
                <td>@v.ProfileImage</td>
                <td>@v.Description</td>
                <td>@v.Email</td>
                <td>@v.EmailConfirmed</td>
                <td>@v.PhoneNumber</td>
                <td>@v.PhoneNumberConfirmed</td>
                <td>@v.UserName</td>
                <td><a href="Adminview/Delete/Id=@v.Id">Delete</a></td>
            </tr>
        }
    </table>

